.container {	border-radius: 0.1rem;	background-color: rgba(255,255,255,0.9);	margin-left: 0.1rem;	width: -webkit-calc(100% - 0.2rem);	}.item {	height: 2.5rem;	padding-left: 0.4rem;}.item:last-of-type{	border-bottom: 0px solid #E6E6E6;}.item:active {	background-color: rgba(80,80,80,0.1);}.item-name {	min-width:3.6rem;	letter-spacing: 0.05rem;	font-size: 0.8rem;}.item-value{	color:#999999; 	font-size: 0.7rem;	margin-right: 0.5rem;}.aui-iconfont {	color:#999999; }.user-header {	height: 4.0rem;	line-height: 4.0rem;}img {	border-radius: 30px;	width: 3.0rem;	height: 3.0rem;}.login-out-btn{	width:-webkit-calc(100% - 2.8rem);	margin-left:1.4rem;	height:2.05rem;	line-height:2.05rem;	margin-top: 1.8rem;	text-align:center;	background-color: rgba(205,176,132,1);	border-radius: 5px;	color: rgba(255,255,255,1);}