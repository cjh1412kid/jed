    function initChart1(_list) {
        $('.skuNum.year').text(_list.skuNum);
        $('.inventory.year').text(_list.stock);
        var str = '';
        var list = [];
        for (var i = 0; i < _list.stockStructure.length; i++) {
            if (_list.stockStructure[i].stock <= 0) {
                continue;
            }
            str += ' <div class="btn" tapmode onclick="chose1(this)">'
            str += '<input class="aui-radio year" type="checkbox" name="radio" onclick="return false;" data-year="' + _list.stockStructure[i].year + '">'
            str += '<span class="type-name">' + _list.stockStructure[i].year + '</span>'
            str += '</div>'
            var obj = {};
            obj.value = _list.stockStructure[i].stock;
            obj.name = _list.stockStructure[i].year + '\n' + _list.stockStructure[i].stock + '\n' + _list.stockStructure[i].percent;
            list.push(obj)
        }
        $('.btns.year').html(str);
        myChart1 = echarts.init(document.getElementById('myChart1'));
        option1 = {
                color: ['#FFC353', '#36E2DF', '#7990FF', '#FF8844', '#FFAC53', '#FF6914', '#ca8622', '#bda29a', '#6e7074', '#546570', '#c4ccd3'],
                // dataset: {
                // 	source: [
                //     ["2016",0.6,1],
                //     ["2017",0.34,10],
                //     ["2018",0.7,20],
                // 	]
                // },
                series: [{
                    hoverAnimation: false,
                    selectedMode: 'multiple',
                    hoverOffset: 10,
                    labelLine: {
                        show: true,
                        length: 10,
                        length2: 10,
                    },
                    label: {
                        // normal: {
                        //     position: 'inner'
                        // }
                    },
                    type: 'pie',
                    radius: 80,
                    center: ["50%", "50%"],
                    data: list
                        // data:[
                        //         // {value:335, name:'2016', selected:true},
                        //         {value:335, name:'2016', selected:true},
                        //         {value:679, name:'2017', selected:true},
                        //         {value:1548, name:'2018'}
                        //     ]
                }]
            },
            myChart1.on('click', function(params) {
                // 控制台打印数据的名称
                var pageParams = params.data.name.split('\n')[0]
                var pageParam = {
                    flag: "goodsList",
                    rangeType: "0",
                    dataType: "0",
                    name: '全部货品',
                    seasonSeqList: [],
                    supplement: '',
                    categorySeqList: [],
                    colorSeqList: [],
                    cateSeqList: [],
                    sXMap: "",
                    fuzzySearchParam: "",
                    sizeType: "",
                    sizeSeqStart: "",
                    sizeSeqEnd: "",
                    orderBy: 1,
                    orderDir: 0,
                    saleTimeStart: "2017/01/02",
                    saleTimeEnd: _getFormatedDate("", "yyyy/MM/dd"),
                    stockMinNum: "",
                    stockMaxNum: "",
                    yearList: [pageParams],
                    timeRange: "30",
                    shopSeqList: []
                };
                api.openDrawerLayout({
                    name: "list_win",
                    url: "goods_list_win.html",
                    pageParam: pageParam,
                    animation: {
                        type: 'push'
                    },
                    softInputMode: "resize",
                    softInputBarEnabled: false,
                    overScrollMode: "always",
                    slidToOpenPane: false,
                    slidToClosePane: false,
                    rightPane: {
                        edge: 60,
                        pageParam: pageParam,
                        softInputMode: "resize",
                        softInputBarEnabled: false,
                        name: 'filter_win',
                        url: '../filter_win.html'
                    }
                });
                console.log($api.jsonToStr(yearList));
                console.log(params.seriesIndex + ',' + params.seriesName + ',' + params.name + ',' + params.dataIndex + ',' + params.data + ',');
            });
        myChart1.setOption(option1);

    }

