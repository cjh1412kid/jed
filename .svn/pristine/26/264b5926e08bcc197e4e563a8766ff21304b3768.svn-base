.flex-style{
  display: flex;
  justify-content: center;
  align-items: center;
}
.flex-style1{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
body,html{
  width: 100%;
  height: 100%;
}
body{
  background-color: rgba(243,243,243,1);
}
.aui-iconfont {
	font-size: 0.6rem !important;
}
.sub-header,.list-item{
  height: 2.0rem;
  line-height: 2.0rem;
	font-size: 0.6rem;
	color: #505050;
}
.index{
  width: 1.25rem;
}
.index1{
  width: .8rem;
}
/*.goods-id{
  overflow-x: scroll;
}*/
.sale-icon{
  width: 2rem;
}
.sale-icon1{
  width: 2rem;
}
.sale-icon2{
  width: 2.8rem;
}
.sale-icon3{
  width: 2.8rem;
}
.list-width{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 1.6rem;
}
.list-width1{
  width: 1.6rem;
}
.list-width2{
  width: 2.8rem;
}
.list-width3{
  width: 2.2rem;
}


.goods-id div{
  width: 5.6rem;
}
.sort-btn{
	font-size: 0.6rem;
}
.sort-btn span{
  font-size: 0.4rem;
}
.sort-btn.active{
	font-size: 0.6rem;
	color: rgba(255,81,0,1);
}
.sort-btn img{
	width: 0.45rem;
	display: block;
}
.count{
  height: 6.65rem;
  background-color: white;
  border-top-left-radius: 0.5rem;
  border-top-right-radius: 0.5rem;
}
.title{
  height:0.98rem;
  font-size:0.7rem;
  font-family:PingFangSC-Semibold;
  font-weight:bold;
  color:rgba(80,80,80,1);
  line-height:0.98rem;
  margin-left: 1.0rem;
  margin-top: 0.63rem;
}
.img{
  width: 5.0rem;
  margin-top: 0.55rem;
  margin-bottom: 0.38rem;
  margin-left: 1.0rem;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;;
  background-image: url(../../image/sale/img_count.png);
  margin-right: 1.13rem;
  /*height: 2.82rem;*/
}
.count-name,.count-value{
  font-size:0.6rem;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(155,155,155,1);
}
.count-value{
  color:rgba(50,50,50,1);
  font-weight: bold;
}
.ground{
  height: 1.1rem;
  /*background-color: red;*/
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;;
  background-image: url(../../image/sale/img_group.png);
}
.list-content{
  background-color: white;
}
.banner{
  width: 100%;
  height: 12rem;
  position: relative;
}
.swiper-container{
  border-radius: .5rem;
  width:100%;
    height:100%;
}
.swiper-wrapper{
  border-radius: .5rem;
}
.swiper-container-horizontal>.swiper-pagination-bullets, .swiper-pagination-custom, .swiper-pagination-fraction{
   bottom:.1rem!important;
}
.swiper-slide {
    background: #fff;
}
.swiper-pagination-bullet{
  width: 1.15rem!important;
  height: .15rem!important;
  border-radius: .5rem!important;
}
.swiper-pagination-bullet-active{
  background: #FE5C00!important;
}
.banner-tittle{
  width: 100%;
  height: 1rem;
}
.tittle-text{

font-size:0.7rem;
font-family:PingFangSC-Semibold;
font-weight:600;
color:rgba(80,80,80,1);

}
.tittle-text1{

font-size:0.6rem;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(255,93,0,1);
text-decoration: underline;
}
.chart{
  /*width: 60%;*/
  height: 100%;
}
.text-list{
  height: 1rem;
  width: 100%;
  /*padding-left: 1rem;*/
}
.text-style{

font-size:0.6rem;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(80,80,80,1);

}
.text-style1{

font-size:0.8rem;
font-family:Arial-Black;
font-weight:bold;
color:#FF6914;

}
.text-style2{

font-size:0.8rem;
font-family:Arial-Black;
font-weight:bold;
color:#505050;

}
.text-style3{
color: #FF0000;
}
.text-style4{
color: #00C84E;
}
.list-content{
  margin: 0 .5rem;
}
.sub-header{
  border-top-left-radius: .5rem;
  border-top-right-radius: .5rem;
  background: #fff;
}
#myChart{
  width: 8.5rem ;
  height: 8.5rem ;
}
#myChart1{
  width: 8.5rem ;
