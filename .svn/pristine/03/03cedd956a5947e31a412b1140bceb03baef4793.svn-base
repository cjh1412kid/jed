    display: flex;
    flex-direction: column;
}
.allShopContent_t1{
  width: 100%;
  height: 1.5rem;
}
.allShopContent_t {
    width: 100%;
    height: 1.5rem;
}
.check-btns{
    font-size: 0.7rem;
    color: rgba(80, 80, 80, 1);
}
.allShopContent_t div {
    width: 20%;
    height: 100%;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
