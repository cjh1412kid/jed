.aui-card-list {
	position: relative;
    margin-bottom: 0;
    background: #ffffff;
}
.aui-collapse-header.aui-active {
    background: #ffffff;
}
.tips{
	color: #979797;
}
.aui-card-list-content {
	padding-left: 0.5rem;
	/*padding-right: 0.5rem;*/
	padding-bottom: 0.5rem;
}
.aui-col-xs-3 {
	width: -webkit-calc(25% - 0.5rem);
	margin-right: 0.5rem;
	height:1.22rem;
	height:1.22rem;
	background:rgba(236,236,236,1);
	border-radius:0.17rem;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(80,80,80,1);
	margin-bottom: 0.5rem;
}
.aui-col-xs-12 {
	width: -webkit-calc(100% - 0.5rem);
	margin-right: 0.5rem;
	height:1.22rem;
	height:1.22rem;
	background:rgba(236,236,236,0);
	border-radius:0.17rem;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(80,80,80,1);
}
.aui-col-xs-3.selected{
	background:rgba(255,186,0,0.24);
	border-radius:0.17rem;
	border:0.03rem solid rgba(255,186,0,1);
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,186,0,1)
}
.aui-col-xs-3.blank{
	background:rgba(255,186,0,0);
	border-radius:0.17rem;
	border:none;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,186,0,1)
}
