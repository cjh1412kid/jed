body {
    background-color: rgba(0, 0, 0, 0);
}

.aui-iconfont {
    font-weight: bold!important;
}

.aui-bar {
    border-top-left-radius: 0.5rem;
    border-top-right-radius: 0.5rem;
}

.aui-bar-nav .aui-title {
    min-height: 2.25rem;
    position: absolute;
    margin: 0;
    text-align: center;
    white-space: nowrap;
    right: 2.5rem;
    left: 2.5rem;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    z-index: 2;
}

.content {
    max-height: -webkit-calc(100% - 3.65rem);
    min-height: 10.5rem;
    background-color: #ffffff;
    overflow-y: scroll;
}

.content::-webkit-scrollbar {
    width: 0px;
    height: 0px;
    visibility: hidden;
}

.aui-card-list {
    position: relative;
    margin-bottom: 0;
    background: #ffffff;
}

.aui-collapse-header.aui-active {
    background: #ffffff;
}

.size, .num {
  font-size: 0.65rem;
}

.tips {
    color: #979797;
}

.aui-card-list-content {
    padding-left: 0.5rem;
    /*padding-right: 0.5rem;*/
    padding-top: 0.5rem;
}

.aui-col-xs-3 {
    width: -webkit-calc(25% - 0.5rem);
    margin-right: 0.5rem;
    height: 1.22rem;
    height: 1.22rem;
    background: rgba(236, 236, 236, 1);
    border-radius: 0.17rem;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
    margin-bottom: 0.5rem;
}

.aui-col-xs-12 {
    width: -webkit-calc(100% - 0.5rem);
    margin-right: 0.5rem;
    height: 1.22rem;
    height: 1.22rem;
    background: rgba(236, 236, 236, 0);
    border-radius: 0.17rem;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

.aui-col-xs-3.selected {
    background: rgba(255, 186, 0, 0.24);
    border-radius: 0.17rem;
    border: 0.03rem solid rgba(255, 186, 0, 1);
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 186, 0, 1)
}

.aui-col-xs-3.blank {
    background: rgba(255, 186, 0, 0);
    border-radius: 0.17rem;
    border: none;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 186, 0, 1)
}

.aui-col-xs-6 {
    width: -webkit-calc(50% - 0.5rem);
    margin-right: 0.5rem;
    height: 1.8rem;
    height: 1.8rem;
    background: rgba(228, 229, 230, 1);
    border-radius: 0.17rem;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
    margin-bottom: 0.5rem;
}

.aui-col-xs-6.selected {
    background: rgba(255, 186, 0, 0);
    border-radius: 0.17rem;
    border: 0.05rem solid rgba(255, 186, 0, 1);
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 186, 0, 1)
}

.aui-col-xs-6.blank {
    background: rgba(255, 186, 0, 0);
    border-radius: 0.17rem;
    border: none;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 186, 0, 1)
}

.aui-col-xs-4 {
    width: -webkit-calc(33.33% - 0.5rem);
    margin-right: 0.5rem;
    height: 1.8rem;
    height: 1.8rem;
    background: rgba(228, 229, 230, 1);
    border-radius: 0.17rem;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
