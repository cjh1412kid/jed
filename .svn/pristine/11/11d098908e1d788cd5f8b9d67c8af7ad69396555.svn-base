                            fontWeight: 900,
                            fontSize: 8,
                            fontFamily: 'Arial-Black',
                        }
                    },

                },

            ]
        }]
    };
    // 使用刚指定的配置项和数据显示图表。
    myChart.setOption(option);

    function clickbtn() {
        $api.css($api.dom('.thisShop1'), 'display:none');
        $api.css($api.dom('.thisShop2'), 'display:none');
        $api.css($api.dom('.thisShop'), 'display:block');
