.listWrap {
    width: 100%;
    height: 24rem;
    overflow-y: scroll;
    padding: .5rem;
}

.priceList {
    width: 100%;
    height: auto;
    background: rgba(255, 255, 255, 1);
    border-radius: 0.75rem;

}

.priceList li:first-child {
    width: 100%;
    height: 1.5rem;
    background: rgba(255, 247, 233, 1);
    border-top-left-radius: .75rem;
    border-top-right-radius: .75rem;
    display: flex;
}
.first{
  width: 50%;
}
.second{
  width: 25%;
}
.priceList li:first-child span {

    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}
.list_Pr{
  width:100%;
height:1.76rem;
display: flex;


}
.list_Pr div{

  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.list_Num{

font-size:0.7rem;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(80,80,80,1);

}
.list_cost{

font-size:0.7rem;
font-family:Arial-Black;
font-weight:900;
color:rgba(151,151,151,1);

}
.priceList li:last-child {
    width: 100%;
    height: 1.76rem;
    padding: 0 .48rem;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 0.5rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}
