/*html, body {
    width: 100%;
    height: 100%;
    overflow: hidden;
}*/

.sal_flex {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.sal_flex1 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.sal_flex2 {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.aui-iconfont {
    font-weight: bold!important;
    color: #505050!important;
}

.salWrap {
    width: 100%;
    flex: 1;
    display: flex;
    flex-direction: column;
}

.tabWrap {
    width: 100%;
    height: 1.92rem;
    overflow-x: scroll;
    margin-bottom: .5rem;
}

/*.aui-bar.aui-bar-nav.aui-bg-theme {
    height: 1.83rem!important;
}*/

#tab {
    width: 46.8rem;
    height: 1.92rem;
    background: none;
}

.aui-tab-item {
    width: 2.2rem;
    margin: 0 .7rem;
    height: 100%;
    line-height: 1.92rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
}

.salMes {
    width: 100%;
    height: auto;
    padding: 0 .5rem;
    margin-bottom: .5rem;
}

.salTittle {
    padding: .3rem .5rem .5rem .5rem;
    width: 100%;
    height: 5.37rem;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0rem 0.5rem 0.5rem -0.5rem rgba(0, 0, 0, 0.15);
    border-radius: 0.5rem;
    display: flex;
    flex-direction: column;
}

.salName {
    width: 100%;
    height: 1rem;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 0.7rem;
    font-family: Arial-Black;
    font-weight: 900;
    color: rgba(80, 80, 80, 1);
    margin-bottom: .3rem;
}

.salContent {
    width: 100%;
    flex: 1;
    display: flex;
}

.content_L {
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-end;
}

.NoSale {
    width: 7.47rem;
    height: 1.68rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.NspeColor1 {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #F5A623;
    font-family: PingFangSC-Semibold;
}

.NspeColor2 {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #288CFF;
    font-family: PingFangSC-Semibold;
}

.Nspe1 {
    font-weight: 800;
    font-size: 1rem;
    transform: translateY(-.1rem);
}

.Nspe2 {
    font-weight: 400;
    font-size: .8rem;
}

.Nsp3 {
    display: flex;
}

.Nsp4 {
    margin-right: .3rem;
}

.isSale {
    width: 5.97rem;
    height: 1.68rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.content_R {
    width: 50%;
    height: 100%;
    padding-left: 2rem;
}

#mychart {
    width: 3.2rem;
    height: 3.2rem;
    margin: auto auto auto 0;
}

.salListWrap {
    width: 100%;
    flex: 1;
    padding: 0 .5rem .5rem .5rem;
}

.salList {
    width: 100%;
    height: 100%;
    position: relative;
    background: rgba(255, 255, 255, 1);
    border-radius: 0.5rem;
    padding: .5rem .5rem 1.65rem .5rem;
    overflow: hidden;
}

.listHeader {
    width: 100%;
    height: 2.6rem;
    padding: 0 3rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(210, 210, 210, 1);
}

.listHeader span {
    width: 33%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.listhot {
    color: #FF5100;
    font-size: .8rem;
}

.listTittle {
    width: 100%;
    height: 2.17rem;
}

.listTittle div {
    width: 25%;
    height: 100%;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.listTittle div:first-child {
    font-size: .7rem;
}

.listThot {
    color: #FF5100!important;
}

.listIco {
    font-size: .5rem;
    margin-left: .1rem;
}

.textStyle {
    font-size: 0.9rem;
    font-family: Arial-Black;
    font-weight: 900;
    color: rgba(151, 151, 151, 1);
}

.textStyle1 {
    font-size: 0.7rem;
    font-family: Arial-Black;
    font-weight: 900;
    color: rgba(151, 151, 151, 1);
}

.aui-content-padded {
    margin: 0;
}

.aui-collapse-header.aui-active {
    background: #fff;
}

.aui-list .aui-list-item {
    padding-left: 0;
}

.aui-list-item-inner>.sal_flex1 {
    width: 25%;
    height: 100%;
}

.item_Spe {
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.item_Spe1 {
    font-size: .7rem!important;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1)!important;
}

.aui-list .aui-list-item-inner {
    padding-right: 0;
}

.aui-list .aui-list-item i.aui-iconfont {
    font-size: .3rem;
    color: #FF5100!important;
    font-weight: bolder!important;
}

.aui-list-item-inner {
    height: 1.4rem;
}

.speList {
    min-height: 2.2rem!important;
}

.weekList {
    width: 100%;
    height: auto;
    padding-left: 1.65rem
}

.weekList li {
    width: 100%;
    height: 1.42rem;
    background: rgba(250, 250, 250, 1);
}

.weeSpe {
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.weeSpe1 {
    font-size: 0.6rem;
    font-family: Arial-Black;
    font-weight: 900;
    color: rgba(151, 151, 151, 1);
}

.weeSpe2 {
    font-size: 0.5rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(151, 151, 151, 1);
}

.weeMove {
    transform: translateX(-.4rem);
}

.weeMove1 {
    transform: translateX(-1rem);
}

.weeMove2 {
    transform: translateX(-1.5rem);
}

.thisShop {
    width: 100%;
    flex: 1;
    display: none;
}

.thisShop1 {
    flex: 1;
    display: none;
}

.thisShop2 {
    flex: 1;
    display: none;
}

.listFooter {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 0.5rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(155, 155, 155, 1);
    padding: 0 .5rem;
}

.footerWrap {
    width: 100%;
    height: 1.65rem;
    position: absolute;
    left: 0;
    bottom: 0;
    background: #fff;
    border-bottom-left-radius: .5rem;
    border-bottom-right-radius: .5rem;
}

.huizong {
    height: 16rem;
    width: 100%;
    overflow-y: scroll;
}
