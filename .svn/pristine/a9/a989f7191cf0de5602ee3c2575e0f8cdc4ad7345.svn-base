/* 页面的播放按钮 */
input{
  display: flex;
  align-items: center;
}
#video {
    width: 2.4rem;
    height: 2.4rem;
    background-color: rgba(0, 0, 0, 0.2);
    border: 1px solid #fff;
    border-radius: 50%;
    position: absolute;
    right: -webkit-calc(50% - 1.2rem);
    bottom: -webkit-calc(50% - 1.2rem);
    z-index: 1;
}


/* 重写vedio.css的播放按钮(隐藏) */

.video-js .vjs-big-play-button {
    display: none;
    //	width: 2.4rem;
    //	height: 2.4rem;
    //	background-color: rgba(0,0,0,0.2);
    //	border: 1px solid #fff;
    //	border-radius: 50%;
    //	position: absolute;
    //	right: -webkit-calc(50% - 1.2rem);
    //	bottom: -webkit-calc(50% - 1.2rem);
    //	z-index: 1;
}


/* 播放器返回按钮 */

.back-btn {
    position: absolute;
    top: 0.5rem;
    right: 0.5rem;
    z-index: 99;
}


/*  商品展示图片容器 */

.goods-images {
    width: 100%;
    min-height: 13.5rem;
    background-color: #ffffff;
}

.aui-slide-wrap .aui-slide-node {
    position: absolute;
    top: 0;
    left: 0;
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    list-style: none;
    overflow: hidden;
    //  background-color: #FF2600;
}

.aui-slide-node img {
    width: auto;
    height: 100%;
    display: block;
    //  background-color: #00BCD4;
    //  padding: 0.5rem;
}


/* 轮播圆点颜色 */

.aui-slide-page-active {
    background: #bc9761;
    from {
        opacity: 0.3;
    }
    to {
        opacity: 1
    }
}

.goods-id-div {
    background-color: white;
    height: 2.2rem;
    line-height: 2.2rem;
    font-weight: bold;
}

.goods-id-div .name {
    font-size: 0.7rem;
}

.goods-id-div .value {
    font-size: 0.8rem;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(80, 80, 80, 1);
}

.recommend {
    width: 3.42rem;
    height: 1.0rem;
    text-align: center;
    line-height: 1.0rem;
    background: rgba(255, 81, 0, 0.1);
    box-shadow: 0rem 0.03rem 0.25rem -0.03rem rgba(0, 0, 0, 0.3);
    border-radius: 0.5rem;
    border: 0.05rem solid rgba(255, 81, 0, 1);
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 81, 0, 1);
    margin-top: 0.6rem;
    margin-right: 0.5rem;
}

.table.header {
    height: 1.52rem;
    background: rgba(241, 241, 241, 1);
    border-top-left-radius: 0.5rem;
    border-top-right-radius: 0.5rem;
}

.table.row {
    height: 1.52rem;
    background: rgba(255, 250, 248, 1);
}

.table.row:nth-of-type(2n+1) {
    background: rgba(248, 252, 255, 1);
}

.table.row:nth-last-of-type(1) {
    border-bottom-left-radius: 0.5rem;
    border-bottom-right-radius: 0.5rem;
}

.goods-infos input {
    height: auto;
    word-wrap: break-word;
    word-break: break-all;
    overflow: hidden;
    height: 1.0rem;
}

.sale-value, .score-value {
    //	color: #666666;
}
.aui-col-xs-6.flex-wrap.aui-margin-t-5{
  display: flex;
  align-items: center;
}
.parameter-name {
    width: 4.0rem;
    text-align: right;
    font-family: PingFang-SC-Regular;
    font-size: 0.6rem;
    color: #999999;

}
.spe-width{
  width: 3.5rem!important;
}
.spe-width1{
    width:5.5rem!important;
}
.parameter-value {
    font-family: PingFang-SC-Regular;
    font-size: 0.6rem!important;
    height: 1.0rem!important;
}

.aui-text-brown {
    color: #bc9761 !important;
}

.score {
    height: 2.2rem;
    background-color: white;
}

.score .name {
    line-height: 2.2rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

.avg-score{
  padding-left: 5rem;
}
.avgScore{

   color:rgba(255,186,0,1);
}
