
/*自定义radio,checkbox大小*/
.aui-radio,
.aui-checkbox {
    margin-top: 0.3rem;
    width: 0.7rem;
    height: 0.7rem;
    float: right;
}
/* 自定义选中状态下radio,checkbox背景色 */
.aui-radio:checked,
.aui-radio.aui-checked,
.aui-checkbox:checked,
.aui-checkbox.aui-checked {
    background-color: rgba(255,81,0,1);
    border: solid 1px rgba(255,81,0,1);
    text-align: center;
    background-clip: padding-box;
}
/* 自定义禁用状态下radio,checkbox背景色 */
.aui-radio:disabled,
.aui-radio.aui-disabled,
.aui-checkbox:disabled,
.aui-checkbox.aui-disabled {
    background-color: #FFFFFF;
    border: solid 1px #dddddd;
}
.aui-disabled.aui-checked{
  background-color: rgba(255,81,0,1);
  border: solid 1px rgba(255,81,0,1);
  text-align: center;
  background-clip: padding-box;
}
