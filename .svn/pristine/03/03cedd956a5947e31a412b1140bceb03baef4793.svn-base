}
.aui-iconfont {
    font-weight: 900!important;
    color: #505050!important;
}

.tabWrap {
    width: 100%;
    height: 1.92rem;
    overflow-x: scroll;
    margin-bottom: .5rem;
    /*position: relative;*/
}

#tab {
    width: 46.8rem;
    height: 1.92rem;
    background: none;
    /*margin-left: -10rem;*/
