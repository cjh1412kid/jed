.arrow-detail {
    display: box;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    box-orient: vertical;
    -webkit-box-align: center;
    box-align: center;
    -webkit-box-pack: center;
    box-pack: center;
    margin-left: 0.5rem;
    margin-right: 0.5rem;
}

.arrow-icon {
    width: 100%;
    height: 0.26rem;
    margin-top: 0.25rem;
    /*margin-bottom: 0.25rem;*/
}

.line-horizontal {
    background-color: white;
    height: 0.075rem;
    margin-right: -0.25rem;
}

.line-oblique {
    background-color: white;
    height: 0.5rem;
    width: 0.075rem;
    transform: rotate(-45deg);
    margin-right: 0.25rem;
    margin-top: -0.35rem;
}

.goods-id {
    font-size: 0.6rem;
