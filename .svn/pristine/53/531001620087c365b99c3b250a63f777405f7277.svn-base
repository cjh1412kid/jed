    function getDatas() {
        var url = window.myServerUrl + "/order/app/targetShop/selectSalesTargetAnalyze";
        url += "?targetYear=" + new Date().getFullYear();
          url += "&shopSeq=" + api.pageParam.shopSeq;
          if (api.pageParam.month!= 0) {
              url += "&targetMonth="+api.pageParam.month;
          }
        console.log("获取门店指标分析接口数据-->url:" + url);
        _showProgress('加载中...');
        _ajax(url, "sale_indicator_win", function(ret, err) {
          api.hideProgress();
            if (ret) {
                console.log("获取门店指标分析接口数据-->url:" + $api.jsonToStr(ret));
                if (ret.code == 0) {
                  shopData = ret.result;

                  console.log($api.jsonToStr(shopData));
                  initUI();
                } else {
                    _toast(ret.msg);
                }
            } else {
                console.log("获取门店指标分析接口数据-->err:" + $api.jsonToStr(err));
                _toast(window.ajaxErrorMessage);
            }
            //callback();
        });
    }
