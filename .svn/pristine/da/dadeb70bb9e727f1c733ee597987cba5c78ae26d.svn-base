html{
	background-color: rgba(0,0,0,0);
}
.category-pop{
	width: 100%;
    height: 100%;
    background-color: #ffffff;
//  box-shadow: 0 0 0.1rem #999999;
}
.category-pop-header{
	height: 2.5rem;
	position: relative;
}
.category-pop-header div{
	width： 25%；
	height:1.2rem;
	line-height:1.2rem;
	padding-left: 0.6rem;
	padding-right: 0.6rem;
	font-size: 0.7rem;
	border: 1px solid #cccccc;
	border-radius: 0.6rem;
}
.category-pop-header div.active{
	background-color: #bc9761;
	border: 1px solid #bc9761;
	color: #FFFFFF;
}
.category-pop-header div{
	margin-left: 0.5rem;
	margin-right: 0.5rem;
}

.category-pop-body{
	overflow-y: auto;
	-webkit-overflow-scrolling: touch;
}
.aui-collapse-header.aui-active {
    background: #FFFFFF!important;
}
.aui-list-item-title{
	font-size: 0.7rem!important;
}
.aui-collapse-content .aui-col-5{
	padding:0.25rem; 
	text-align: center;
	font-size: 0.7rem;
}

.blank-area{
	background-color: rgba(0,0,0,0);
}

.aui-collapse-content .aui-col-5:active{
	background-color: #bc9761;
	color: #ffffff;
	border-radius: 0.6rem;
}
.aui-bg-gray{
	background-color: #F5F5F5 !important;
}
