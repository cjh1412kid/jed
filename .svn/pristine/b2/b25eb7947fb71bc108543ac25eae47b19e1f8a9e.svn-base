body{
	background-color: #ffffff;
}
.aui-iconfont {
	font-weight: bold!important;
}

.content{
	background-color: #ffffff;
	overflow-y: scroll;
}
.aui-card-list {
	position: relative;
    margin-bottom: 0;
    background: #ffffff;
}
.aui-collapse-header.aui-active {
    background: #ffffff;
}
.tips{
	color: #979797;
}
.aui-card-list-content {
	padding-left: 0.5rem;
	/*padding-right: 0.5rem;*/
	padding-bottom: 0.5rem;
}
.aui-col-xs-3 {
	width: -webkit-calc(50% - 0.5rem);
	margin-right: 0.5rem;
	height:1.22rem;
	height:1.22rem;
	background:rgba(236,236,236,1);
	border-radius:0.17rem;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(80,80,80,1);
	margin-bottom: 0.5rem;
}
.aui-col-xs-4 {
	width: -webkit-calc(75% - 0.5rem);
	margin-right: 0.5rem;
	height:1.22rem;
	height:1.22rem;
	background:rgba(236,236,236,1);
	border-radius:0.17rem;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(80,80,80,1);
	margin-bottom: 0.5rem;
}
.aui-col-xs-12 {
	width: -webkit-calc(100% - 0.5rem);
	margin-right: 0.5rem;
	height:1.22rem;
	height:1.22rem;
	background:rgba(236,236,236,0);
	border-radius:0.17rem;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(80,80,80,1);
}
.aui-col-xs-3.selected{
	background:rgba(255,186,0,0.24);
	border-radius:0.17rem;
	border:0.03rem solid rgba(255,186,0,1);
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,186,0,1)
}
.aui-col-xs-4.selected{
	background:rgba(255,186,0,0.24);
	border-radius:0.17rem;
	border:0.03rem solid rgba(255,186,0,1);
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,186,0,1)
}
.aui-col-xs-3.blank{
	background:rgba(255,186,0,0);
	border-radius:0.17rem;
	border:none;
	font-size:0.6rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,186,0,1)
}
.graphTime{
  width: -webkit-calc(100% - 0.5rem);
	margin-right: 0.5rem;
  height:1.22rem;
  font-size:0.6rem;
  font-family:PingFangSC-Regular;
  font-weight:500;
  color:#FFBA00;
  display: -webkit-flex;
  display: -ms-flex;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.graphTime div:nth-child(2){
font-size:0.5rem;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(151,151,151,1);
}

.aui-bar-tab .aui-bar-tab-item {
	display: table-cell;
	position: relative;
    width: 1%;
    height: 2.5rem;
    text-align: center;
    vertical-align: middle;
	opacity: 0.8;
}
footer{
	height:2.82rem;
	background:rgba(255,255,255,1);
}
.btns{
	width:7.12rem;
	height:1.5rem;
	line-height: 1.5rem;
	background:linear-gradient(270deg,rgba(255,170,0,1) 0%,rgba(255,113,0,1) 100%);
	border-radius:0.89rem;
	font-size:0.7rem;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(255,255,255,1);
}
.reset{
	background-color: rgba(255,73,120,1);
	border-top-left-radius:0.89rem;
	border-bottom-left-radius:0.89rem;
}
