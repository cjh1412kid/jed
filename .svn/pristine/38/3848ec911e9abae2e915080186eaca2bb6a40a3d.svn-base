.card {
    /*background: linear-gradient(144deg, rgba(97, 196, 250, 1) 0%, rgba(68, 143, 255, 1) 100%);*/
    border-radius: 0.25rem;
    height: 7.98rem;
    background-image: url("../image/bg_cargo.png");
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.dirction {
    height: 2.63rem;
    margin-bottom: 0.25rem;
}

.from, .to {
    display: box;
    display: -webkit-box;
    -webkit-box-align: center;
    box-align: center;
    -webkit-box-pack: center;
    box-pack: center;
}

.icon-img .from, .icon-img .to {
    -webkit-box-align: end;
    box-align: end;
    margin-bottom: 0.5rem;
}

.dirction .name {
    width: 3.73rem;
    height: 1.22rem;
    line-height: 1.22rem;
    background: linear-gradient(247deg, rgba(17, 0, 213, 0.0742) 0%, rgba(0, 76, 92, 0.0742) 100%) rgba(0, 0, 0, 0.0742);
    border-radius: 0.61rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    text-align: center;
}

.icon {
    width: 1.72rem;
    height: 1.72rem;
    margin-left: 0.9rem;
    margin-right: 0.9rem;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.from .icon {
    background-image: url("../image/icon_cargo_out.png");
}

.to .icon {
    background-image: url("../image/icon_cargo_in.png");
}

.goods-image img {
    width: 3.8rem;
    height: 3.8rem;
    box-shadow: 0 0 0.5rem 0.25rem rgba(255, 255, 255, 0.5);
    border-radius: 0.25rem;
    border: none;
    font-size: 0.5rem;
    text-align: center;
    line-height: 3.8rem;
    color: white;
}

