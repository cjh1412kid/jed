<script type="text/javascript">
    apiready = function() {
        api.parseTapmode();
        var header = $api.byId('aui-header');
        $api.fixStatusBar(header);
        $('.shop').triggerHandler('click')
        $api.css($api.dom('.thisShop'), 'display:block');
    }
    var tab = new auiTab({
        element: document.getElementById("tab"),
    }, function(ret) {
        console.log(ret)
    });
    var collapse = new auiCollapse({
        autoHide: false //是否自动隐藏已经展开的容器
    });
    var myChart = echarts.init(document.getElementById('mychart'));

    // 指定图表的配置项和数据
    option = {

        color: ['#F0F0F0', '#288CFF'],
        series: [{
            name: '访问来源',
            type: 'pie',
            radius: ['70%', '100%'],
            avoidLabelOverlap: false,
            hoverOffset: 3,
            startAngle: 180,
            label: {
                normal: {
                    show: false,
                    position: 'center'
                },
                emphasis: {
                    show: true,
                    textStyle: {
                        fontSize: '30',
                        fontWeight: 'bold'
                    }
                }
            },
            labelLine: {
                normal: {
                    show: false
                }
            },
            data: [{
                    value: 20,
                    emphasis: {
                        label: {
                            formatter: '完成率\n{d}%',
                            color: '#288CFF',
                            fontWeight: 900,
                            fontSize: 8,
                            fontFamily: 'Arial-Black',
                        }
                    },



                }, {
                    value: 30,
                    emphasis: {
                        label: {
                            formatter: '未完成率\n{d}%',
