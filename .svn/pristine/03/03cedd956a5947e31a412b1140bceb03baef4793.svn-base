}

.aui-tab-item {
    width: 2.2rem;
    margin: 0 .7rem;
    height: 100%;
    line-height: 1.92rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
}

/*卡片部分样式*/
.salMes {
    width: 100%;
    height: auto;
    padding: 0 .5rem;
    margin-bottom: .5rem;
}

.salTittle {
    padding: .5rem .5rem .5rem .5rem;
    width: 100%;
    height: 10rem;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0rem 0.5rem 0.5rem -0.5rem rgba(0, 0, 0, 0.15);
    border-radius: 0.5rem;
    display: flex;
    flex-direction: column;
}

.salName {
    width: 100%;
    height: 1rem;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 0.7rem;
    font-family: Arial-Black;
    font-weight: 900;
    color: rgba(80, 80, 80, 1);
    margin-bottom: .3rem;
}

.salContent {
    width: 100%;
    flex: 1;
    display: flex;
}

.content_L {
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-end;
}

