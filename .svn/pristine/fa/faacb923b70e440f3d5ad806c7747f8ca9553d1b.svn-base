.left-btn{
	width: 1.4rem;
	height: 1.4rem;
}
.search-are{
	width: 100%;
	height: 1.4rem;
	background-color: #f5f5f5;
	border-radius: 0.7rem;
	border: solid 0rem #f5f5f5;
//	border-bottom: solid 0.05rem #f5f5f5;
	margin-top: 0.65rem;
}
.search-are img{
	width: 1.4rem;
	height: 1.4rem;
	padding:0.4rem;
//	background-color: #0062CC;
}
.icon-search{
	font-size:0.7rem;
	font-weight: bold; 
}
.search-input{
	height: 1.4rem;
	line-height:1.5rem;
	font-size: 0.6rem;
	color: #999999;
//	background-color: #00897B
}
.aui-bar {
	background-color: rgba(255,255,255,1) !important;
	min-height: 2.5rem;
}
.aui-bar-nav {
	line-height: 2.5rem!important;
}
.aui-bar-nav .aui-title {
	min-height: 2.5rem!important;
}
.aui-bar-nav .aui-btn {
	height: 2.5rem;
	line-height: 2.5rem;
}
.aui-searchbar {
	background: transparent;
	height:2.2rem;
	margin-left: 0px !important;
}
#search-input{
	color: #212121!important;
	font-size: 0.75rem;
}
#search-input::-webkit-input-placeholder{
	color: #999999;
	font-size: 0.75rem;
}
.aui-searchbar-clear-btn {
	position: absolute;
	right: 5px;
	top: 3px;
	width: 1.1rem;
	height: 1.1rem;
	background: transparent;
	border-radius: 50%;
	line-height: 0.6rem;
	text-align: center;
	display: none;
}
.aui-searchbar-clear-btn .aui-iconfont {
	font-size: 0.5rem;
	margin: 0 auto;
	position: relative;
	top: -2px;
	color: #bbbbbb;
	font-weight: bold;
}
.aui-bar-nav .aui-searchbar-input {
	background-color: #f6f6f6 !important;;
	padding-left:0.5rem;
	height: 1.2rem;
	border-radius: 0.6rem;
	border: solid 0.017rem #444444;
}
.aui-searchbar .aui-searchbar-btn {
	color: rgba(80,80,80,1);
	font-size: 0.6rem;
	text-shadow: 0px 0px 1px 1px #FFFFFF;
	padding-left: 2px;
	padding-right: 2px;
	height: 0.7rem;
	line-height:0.7rem;
	border: solid 0.05rem rgba(0,0,0,0.2);
	border-radius: 0.25rem;
	background-color: #FFFFFF;
}
.aui-searchbar .aui-iconfont {
	/*color: #9e9e9e !important;*/
	color: #212121 !important;
}
.list-area{
//	width:-webkit-calc(100% - 3.26rem);
//	margin-left: 1.63rem;
}
.list-item{
	height: 0.6rem;
	line-height: 0.6rem;
	margin-bottom: 0.98rem;
	width:-webkit-calc(100% - 3.26rem);
	margin-left: 1.63rem;
}
.item-name{
	width:4.5rem;
	color: #6c6c6c;
}
.item-value{
	color: #1d212c;
}
.shop-status{
	margin-left: 0.25rem;
	background-color: #00ee00;
	color: #fff;
	padding-left: 0.25rem;
	padding-right: 0.25rem;
	border-radius: 0.25rem;
	padding-top: 0.1rem;
	font-size: 0.6rem;
}
.item-icon{
	color: #6c6c6c;
	font-weight: bold;
}
.item-imgs{
	width:-webkit-calc(100% - 0.25rem);
	margin-left: 0.25rem;
}
.item-img{
	width: -webkit-calc(50% - 0.22rem);
	margin-bottom:0.17rem;
	height: 4.97rem;
	background-color: #6c6c6c;
	border-radius: 0.33rem;
	border: solid 0.02rem #7b7b7b;
	float: left;
}
.item-img:nth-of-type(odd){
	margin-right: 0.22rem;
}
.refreshBtn {
	position: absolute;
	right: 0.5rem;
	bottom: 0.5rem;
	width: 1.5rem;
	height: 1.5rem;
	border-radius: 0.75rem;
	background-color: rgb(255, 255, 255);
	border: 0.05rem solid rgba(80,80,80,0.3);
}