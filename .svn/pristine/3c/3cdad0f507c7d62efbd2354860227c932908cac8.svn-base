.aui-bar-light {
    color: #03a9f4;
    background-color: #ffffff;
    border-bottom: 0px solid #dddddd;
}

@media screen and (-webkit-min-device-pixel-ratio:1.5) {
    .aui-bar.aui-bar-light {
        border: none;
        background-size: 100% 0px;
        background-repeat: no-repeat;
        background-position: bottom;
        background-image: linear-gradient(0, #dddddd, #dddddd 50%, transparent 50%);
        background-image: -webkit-linear-gradient(90deg, #dddddd, #dddddd 50%, transparent 50%);
    }
}

.aui-iconfont {
    font-weight: bold !important;
}

.aui-pull-right img {
    //	background-color: #0062CC;
    width: 0.98rem;
    height: 0.87rem;
}

.aui-iconfont {
    background-position: center;
    background-repeat: no-repeat;
    background-size: 1.3rem 1.3rem;
    color: rgba(0, 0, 0, 0);
}

.poster-area {
    height: 10.1rem;
    background-color: #FFFFFF;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    padding-bottom: 0.5rem;
}


/* 轮播圆点颜色 */

.aui-slide-page-active {
    background: #bc9761;
    from {
        opacity: 0.3;
    }
    to {
        opacity: 1
    }
}


/*宫格布局*/

.aui-grid {
    width: 100%;
    background-color: white;
    display: table;
    table-layout: fixed;
    padding: 0.0rem;
}

.title-divider {
    margin: 0.5rem;
}

.aui-grid .title {
    height: 0.85rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Medium;
    font-weight: bold;
    color: rgba(80, 80, 80, 1);
    line-height: 0.98rem;
}

.aui-grid .divider {
    height: 0.85rem;
    margin-left: 0.25rem;
}

.aui-grid [class*=aui-col-] {
    display: table-cell;
    position: relative;
    text-align: center;
    vertical-align: middle;
    padding: 0!important;
}

.aui-grid [class*=aui-col-]:active {
    background-color: #f5f5f5!important;
}

.aui-grid [class*=aui-col-xs-]:active {
    background-color: #f5f5f5;
}

.my-grid img {
    position: relative;
    z-index: 20;
    top: 0.45rem;
    height: 2.0rem;
    width: 2.0rem;
    display: inline-block;
}

.aui-grid .aui-grid-label {
    display: block;
    position: relative;
    margin-top: 0.32rem;
    margin-bottom: 0.5rem;
    font-size: 0.6rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

.aui-grid .aui-badge {
    position: absolute;
    top: 0.15rem;
    left: 60%;
    z-index: 99;
    width: 0.72rem;
    height: 0.72rem;
    border-radius: 50%!important;
    background: rgba(255, 0, 0, 1);
    border: 0.07rem solid rgba(243, 243, 243, 1);
    font-size: 0.55rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

.company-recommend, .new-recommend, .all-recommend {
    /*padding-left:0.5rem;
	padding-right:0.5rem;*/
    background-color: #f3f3f3;
}

.category-name {
    /*margin-top:0.82rem;*/
    padding-top: 1.0rem;
    padding-left: 0.5rem;
    padding-right: 0.5rem;
    padding-bottom: 0.5rem;
    font-size: 0.7rem;
    font-family: PingFangSC-Medium;
    font-weight: bold;
    color: rgba(80, 80, 80, 1);
}

.category-name.gszt {
    padding-top: 0.82rem;
    background: -webkit-gradient(linear, 0 0, 0 100%, from(white), to(#f3f3f3));
}

.title-tips {
    font-size: 0.5rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(173, 173, 173, 1);
    line-height: 0.7rem;
}

.title-tips i {
    font-size: 0.6rem !important;
    color: #999999!important;
}

.aui-col-xs-4 {
    //	border: 1px solid #eeeeee;
    text-align: right;
    position: relative;
}

.aui-col-xs-4 .NO {
    position: absolute;
    font-size: 0.5rem;
    min-width: 1.2rem;
    text-align: center;
    float: none;
    right: 0.25rem;
    top: 0.25rem;
    color: #FFFFFF;
    background-color: #bc9761;
    border-radius: 0.1rem;
    opacity: 0.8;
}

.aui-col-xs-4 img {
    width: 100%;
    //	padding: 0.25rem;
    //	background-color: #0062CC;
    //	height:80px;
}

#aui-slide1 .aui-slide-wrap, #aui-slide2 .aui-slide-wrap {
    border-radius: 0.5rem;
}

#aui-slide1 .aui-slide-wrap {
    box-shadow: 2px 4px 0.5rem #ccc;
}

.aui-slide-node, .aui-slide-node img {
    border-radius: 0.5rem;
}

.layout-custom img {
    display: inline !important;
    vertical-align: middle;
}

.aui-content {
    border-radius: 0.5rem;
}


/**
 * 厂家推荐
 */

.all-recommend .aui-grid {
    background-color: #f3f3f3;
    width: -webkit-calc(100% - 1.0rem);
    /*background-color: red;*/
    display: table;
    table-layout: fixed;
    margin-left: 0.5rem;
}

.aui-col-xs-6 {
    width: -webkit-calc(50% - 0.25rem);
    background-color: #fff;
    padding: 0.0rem !important;
    border-radius: 0.5rem;
}

.all-recommend .aui-col-xs-6:active {
    background-color: #ffffff!important;
    opacity: 0.8;
}


/* 商品介绍 (grid)*/

.goods-title-2 {
    height: 1.0rem;
    max-height: 1.0rem;
    display: -webkit-box;
    font-size: 0.7rem !important;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    margin-top: 0.5rem;
}

.score {
    width: 2rem;
    margin-top: 0.2rem;
    padding-left: 0.25rem;
    padding-right: 0.25rem;
    height: 0.75rem;
    background: rgba(255, 186, 0, 1);
    border-radius: 0.35rem;
    font-size: 0.5rem!important;
    font-family: Arial-Black;
    font-weight: 900!important;
    color: rgba(255, 255, 255, 1);
}


/* 商品属性(价格, 货号, 订货量, 评分) */

.goods-attr {
    font-size: 0.6rem;
    color: #999999;
}

.attr-name, .attr-value {
    font-size: 0.6rem;
    color: #999999;
}


/* 厂家推荐商品图片容器 */

.contain {
    width: -webkit-calc(50vw -0.25rem);
    /*屏幕宽度的一半*/
    height: -webkit-calc(50vw -0.25rem);
    /*屏幕宽度的一半*/
}

.item {
    /*float: left;*/
    position: relative;
    /*这里显示的就是1:1,以后div就会保持这个比例*/
    width: 100%;
    padding-bottom: 100%;
}

.item .box {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    text-align: center;
    position: absolute;
}

.item .box img {
    border-top-left-radius: 0.5rem;
    border-top-right-radius: 0.5rem;
}


/* 底部提示 */

.aui-card-list-footer {
    min-height: 1.6rem;
    color: #bc9761;
    padding: 0!important;
}

.aui-card-list-footer i {
    font-weight: bold;
    color: #bc9761;
}

.top {
    width: 2.0rem;
    height: 2.0rem;
    /*background-image: url("../../image/back_top.png");*/
    border-radius: 1.0rem;
    position: fixed;
    right: 1.0rem;
    bottom: 1.0rem;
}


