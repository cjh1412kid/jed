.express-carrynum{
  width: 5rem!important;
}
.reasons{
  position: relative;
}
.reason{
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.reason-content{
  width: 100%;
  height: auto;
  background: #fff;
  position: absolute;
  left: 0;
  bottom: -6rem;
  opacity: 1;
  z-index: 999;
}
.reason-content div{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 2rem;
  width: 100%;
}
.contents{
   overflow-y: scroll;
   padding:0 .5rem .5rem .5rem;
}
.good-intersection{
    width: 100%;
    height: 3.8rem;
    padding: .25rem .5rem;
    background: #fff;
    border-top:0.03rem solid rgba(218,234,255,1);
    border-bottom:0.03rem solid rgba(218,234,255,1);
}
.good-intersection img{
  width: 3.3rem;
  height: 3.3rem;
  margin-right: .5rem;
}
.intersection>div{
  height: 33.33%;
  width: 100%;
  display: flex;
}
.intersection-title{
justify-content: space-between;
align-items: center;
font-size:0.7rem;
font-family:PingFangSC-Medium,PingFang SC;
font-weight:500;
color:rgba(51,51,51,1);

}
.intersection-size{

font-size:0.6rem;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(102,102,102,1);

}
.sizeandnum{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.titles{
  display: flex;
  justify-content: center;
  align-items: center;
}
.sizeandnum>div{
  width: 12.5%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btns{
  border-bottom-left-radius: .25rem;
  border-bottom-right-radius: .25rem;
  overflow: hidden;
}
