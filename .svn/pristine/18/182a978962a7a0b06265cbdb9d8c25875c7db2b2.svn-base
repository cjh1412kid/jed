body,html{
  width: 100%;
  height: 100%;
}
._flex {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

._flex1 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.aui-btn img {
    width: 1.05rem;
    height: 1.05rem;
}

.tittle {
    width: 100%;
    height: 2.07rem;
}

.tittle span {
    width: 33.33%;
    height: 100%;
    font-size: 0.7rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(80, 80, 80, 1);
}

.content {
  overflow-y: scroll;
    padding: 0 .5rem;
}
.shopContent{
  width: 100%;
  height: auto;
  border-radius: .5rem;
  background: rgba(255,255,255,1);

  margin-bottom: .5rem;
}
